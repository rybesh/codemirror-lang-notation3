{
  "compilerOptions": {
    "declaration": true,
    "emitDeclarationOnly": true,
    "forceConsistentCasingInFileNames": true,
    "module": "es2020",
    "moduleResolution": "nodenext",
    "newLine": "lf",
    "plugins": [{"name": "typescript-eslint-language-service"}],
    "strict": true,
    "target": "es2020"
  },
  "include": ["src/*.ts"]
}
